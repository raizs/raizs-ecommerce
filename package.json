  "dependencies": {
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "express": "^4.16.2",
    "file-loader": "^2.0.0",
